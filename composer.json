{
  "name": "nplesa/observer",
  "description": "Self-hosted Laravel Observer component",
  "type": "library",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^12.0",
    "livewire/livewire": "^3.0",
    "reverb/reverb-php": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "nplesa\\Observer\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "nplesa\\Observer\\ObserverServiceProvider"
      ]
    }
  }
}
